<%= form_for(:sessions, url: login_path) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email, type: 'email' %>
    <%= f.label :password %>
    <%= f.text_field :password, type: 'password' %>
    <%= f.submit "#{@name_btn}" %>
<% end %>
<p>Create new account? <%= link_to "Sign up now!", signup_path %></p>