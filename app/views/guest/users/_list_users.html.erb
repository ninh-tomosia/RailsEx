<tr>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.phone %></td>
    <td><%= user.address %></td>
    <td>
        <%= link_to  "Show", user %> | 
        <%= link_to "Edit", edit_user_path(user) %> | 
        <%= link_to "Delete", user, method: :delete, data: { confirm: "Are you sure you want to delete this user?"} %>
    </td>
</tr>